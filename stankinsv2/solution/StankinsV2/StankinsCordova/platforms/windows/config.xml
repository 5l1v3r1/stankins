<?xml version='1.0' encoding='utf-8'?>
<widget id="StankinsCordova.msprogrammer.serviciipeweb.ro" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <name>StankinsAlive</name>
    <description>
        Stankins Alive monitor.
    </description>
    <author email="ignatandrei@yahoo.com" href="http://http://msprogrammer.serviciipeweb.ro">
        Andrei Ignat
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
